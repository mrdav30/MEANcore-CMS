    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-hbs": "^2.3.4",
    "express-session": "^1.17.0",
    "fs-extra": "^9.0.1",
    "generate-password": "^1.5.0",
    "glob": "^7.1.6",
    "googleapis": "^59.0.0",
    "got": "^11.6.0",
    "helmet": "^4.1.0",
