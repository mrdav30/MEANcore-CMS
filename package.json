{
  "name": "meancore",
  "description": "Full-Stack JavaScript  with MongoDB, Express, AngularJS, and Node.js.",
  "version": "0.5.0-beta",
  "private": false,
  "author": "david.oravsky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/meancore/meancore.git"
  },
  "engines": {
    "node": ">=14.9.0",
    "npm": ">=6.14.0"
  },
  "type": "module",
  "scripts": {
    "ng": "ng",
    "preinstall": "npm run bundle:packages",
    "config": "node ./tools/set-env.js",
    "bundle:packages": "node ./tools/bundle-packages.js",
    "link:install": "node ./tools/link-packages.js --place install --package %npm_config_pkg% --depType %npm_config_dep_type% --mod %npm_config_mod% && npm run bundle:packages",
    "link:uninstall": "node ./tools/link-packages.js --place uninstall --package %npm_config_pkg% --depType %npm_config_dep_type% --mod %npm_config_mod% && npm run bundle:packages",
    "bundle:modules": "node ./tools/bundle-modules.js",
    "build:dev": "npm run config && npm run bundle:modules && ng build",
    "build:prod": "npm run config && npm run bundle:modules && ng build -c production",
    "client:dev": "ng serve --aot --progress=false",
    "client:prod": "ng serve -c production --progress=false",
    "server:dev": "nodemon server.js",
    "server:prod": "node server.js",
    "start:dev": "npm run build:dev && npm-run-all -p client:dev server:dev",
    "start:prod": "npm run build:prod && npm run server:prod",
    "test:client": "ng test",
    "lint:client": "ng lint",
    "lint:server": "./node_modules/.bin/eslint ./config/**/*.js ./modules/**/server/**/*.js",
    "e2e": "npm run config && npm run bundle:modules && ng e2e",
    "generate-ssl-certs": "./tools/generate-ssl-certs.sh",
    "seed": "node ./tools/seed-db.js"
  },
  "dependencies": {
    "@angular/animations": "^10.0.12",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.12",
    "@angular/compiler": "^10.0.12",
    "@angular/core": "^10.0.12",
    "@angular/forms": "^10.0.12",
    "@angular/localize": "^10.0.12",
    "@angular/platform-browser": "^10.0.12",
    "@angular/platform-browser-dynamic": "^10.0.12",
    "@angular/router": "^10.0.12",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "agenda": "^3.1.0",
    "async": "^3.1.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "concat-stream": "^2.0.0",
    "connect-flash": "~0.1.1",
